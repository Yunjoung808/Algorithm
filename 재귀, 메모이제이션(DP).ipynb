    "n=7\n",
    "\n",
    "def DFS(m):\n",
    "    if dy[m]>0:\n",
    "        return dy[m]\n",
    "    \n",
    "    if m == 1 or m == 2:\n",
    "        return m\n",
    "    else:\n",
    "        dy[m]=DFS(m-1)+DFS(m-2)\n",
    "        return dy[m]\n",
    "    \n",
    "if __name__==\"__main__\":\n",
    "    dy=[0]*(n+1)\n",
    "    print(DFS(n))\n",
    "    "
